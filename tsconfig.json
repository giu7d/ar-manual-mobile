  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "target": "ES2015",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
